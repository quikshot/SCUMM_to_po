msgid "Ven aquí!"
msgstr ""

#. stringId:1834_
#. Ah-ha!
msgctxt "Room: 030 Script 0204"
msgid "Ah-ha!"
msgstr ""

#. stringId:1835_
#. Caught you, you little thief!
msgctxt "Room: 030 Script 0204"
msgid "Te pillé, pequeño ladronzuelo!"
msgstr ""

#. stringId:1836_
#. Caught you again, you little rat!\255\003
msgctxt "Room: 030 Script 0204"
msgid "Te pillé de nuevo, pequeña rata!"
msgstr ""

#. stringId:1837_
#. Don't you ever learn?
msgctxt "Room: 030 Script 0204"
msgid "Nunca aprenderás, ¿Verdad?"
msgstr ""

#. stringId:1838_
#. Okay, cough up the dough.

#. 1:street
#. 2:carrer
msgctxt "Room: 032 Object name"
msgid "calle@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@"
msgstr ""

#. stringId:2235_
#. 1:Ooooh!  A circus./wait/
#. 2:Ooooh!  Un circ.
msgctxt "Room: 032 Text related to object: cartel"
msgid "Ooooh! Un circo."
msgstr ""

#. stringId:2236_
#. 1:I love a circus!
#. 2:M'encanta un circ!
msgctxt "Room: 032 Text related to object: cartel"
msgid "Me encantan los circos!"
msgstr ""

